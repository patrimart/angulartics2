{
  "extends": "./tsconfig-build.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "../../../../dist/mixpanel/es5",
    "declaration": false,
    "removeComments": true
  },
  "files": [
    "angulartics2-mixpanel.ts",
    "angulartics2-mixpanel-async.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableSummariesForJit": false,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "angulartics2/mixpanel"
  }
}
